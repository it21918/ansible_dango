[Unit]
Description=gunicorn daemon
After=network.target

[Service]
User=jenkins
Group=www-data
WorkingDirectory=/home/jenkins/adminSystem/base
ExecStart=/home/jenkins/adminSystem/myvenv/bin/gunicorn --access-logfile - --workers 3  --bind unix:/home/jenkins/adminSystem/base/base.sock base.wsgi:application


[Install]
WantedBy=multi-user.target
